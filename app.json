{
  "expo": {
    "name": "Movie Search",
    "slug": "movie-search-app",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2196F3",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moviesearch.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to find nearby movie theaters."
      }
    },
    "android": {
      "package": "com.moviesearch.app",
      "adaptiveIcon": {
        "backgroundColor": "#2196F3"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "2de5acd9-5b72-4c8d-830d-e92132ebfddc"
      }
    }
  }
}
